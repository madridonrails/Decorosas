<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN""http://www.w3.org/TR/html4/loose.dtd"><html>  <head>    <title><%= t'head.title' %></title>    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>    <%= stylesheet_link_tag 'type', :media=>'all' %>    <%= stylesheet_link_tag 'structure', :media=>'all' %>    <%= javascript_include_tag :defaults %>  </head>  <body>    <div id="main">      <div id="page">        <%= render :partial => '/layouts/page_parts/logout' if logged_in? %>        <div id="header">          <%= image_tag'logo.png', :alt => t('header.logo.alt')%>        </div>        <%= render :partial => '/layouts/page_parts/navigation' %>        <%= @content_for_layout %>      </div>      <div id="footer" class="footer-text"><a rel="license" target="_blank" href="http://www.opensource.org/licenses/mit-license.php">MIT License</a> . Powered by <a href="http://www.madridonrails.com" target="_blank" >MadridOnRails</a></div>    </div>  </body></html>